{
  "name": "gatsby-source-flamelink",
  "version": "1.2.1",
  "description": "Gatsby source plugin for building websites using the Flamelink CMS as a data source",
  "homepage": "https://github.com/flamelink/gatsby-source-flamelink#readme",
  "bugs": {
    "url": "https://github.com/flamelink/gatsby-source-flamelink/issues"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "flamelink",
    "headless",
    "cms",
    "firebase",
    "firestore"
  ],
  "author": "JP Erasmus <jperasmus11@gmail.com>",
  "main": "index.js",
  "license": "MIT",
  "repository": "https://github.com/flamelink/gatsby-source-flamelink",
  "deprecated": false,
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "dev": "babel -w src --out-dir . --ignore **/__tests__",
    "lint": "eslint ./src --cache"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "compose-then": "^1.0.1",
    "debug": "^4.1.1",
    "firebase-admin": "^8.4.0",
    "flamelink": "^1.0.0-alpha.25",
    "lodash": "^4.17.15",
    "pascalcase": "^1.0.0",
    "present": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "cross-env": "^5.2.0",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "gatsby-source-filesystem": "^2.1.18",
    "jest": "^24.9.0",
    "prettier": "^1.18.2"
  },
  "peerDependencies": {
    "gatsby": "^2.14.0",
    "gatsby-source-filesystem": "^2.1.18"
  }
}
